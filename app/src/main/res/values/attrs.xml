<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NekTextField">
        <attr name="hint" format="string" />
        <attr name="text" format="string" />
        <attr name="icon_nek" format="integer" />
    </declare-styleable>
    <declare-styleable name="NekTxtView">
        <attr name="label" format="string" />
        <attr name="data" format="string" />
        <attr name="img" format="integer" />
        <attr name="showNav" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="OptionView">
        <attr name="name" format="string"/>
        <attr name="useCheckbox" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CountChoosenView">
        <attr name="title" format="string"/>
        <attr name="count" format="integer"/>
    </declare-styleable>
    <declare-styleable name="ChooseLanguageView">
        <attr name="name_lang" format="string"/>
        <attr name="isChecked" format="boolean" />
    </declare-styleable>
</resources>